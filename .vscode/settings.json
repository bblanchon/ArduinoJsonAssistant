{
  "files.eol": "\n",
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, yarn.lock",
    "*.js": "${capture}.spec.js"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}